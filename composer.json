{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "api-platform/api-pack": "^1.2",
        "dunglas/doctrine-json-odm": "^0.1.3",
        "hautelook/alice-bundle": "^2.5",
        "ramsey/uuid-doctrine": "^1.5.0",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/console": "^4.3.5",
        "symfony/css-selector": "^4.3.5",
        "symfony/dom-crawler": "^4.3.5",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^4.3.5",
        "symfony/http-client": "^4.3.5",
        "symfony/lock": "^4.3.5",
        "symfony/mailer": "^4.3.5",
        "symfony/mercure-bundle": "*",
        "symfony/monolog-bundle": "*",
        "symfony/notifier": "^5.0.0@dev",
        "symfony/orm-pack": "^1.0.7",
        "symfony/slack-notifier": "^5.0.0@dev",
        "symfony/translation": "*",
        "symfony/validator": "^4.3.5",
        "symfony/webpack-encore-bundle": "^1.6",
        "symfony/yaml": "^4.3.5"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "*",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "vimeo/psalm": "^3.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "reset": [
            "bin/console doctrine:schema:drop --force",
            "bin/console doctrine:schema:create",
            "bin/console heimdall:lock:create-table -d",
            "bin/console hautelook:fixtures:load"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^4.3.5"
        }
    }
}
