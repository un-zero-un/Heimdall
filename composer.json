{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "api-platform/core": "^2.6",
        "composer/package-versions-deprecated": "1.11.99.5",
        "doctrine/annotations": "^1.0",
        "doctrine/common": "^2",
        "doctrine/doctrine-bundle": "^2",
        "doctrine/doctrine-migrations-bundle": "^2",
        "doctrine/orm": "^2",
        "dunglas/doctrine-json-odm": "^1.0.1",
        "easycorp/easyadmin-bundle": "^2.3.9",
        "gedmo/doctrine-extensions": "^2.4.41",
        "gesdinet/jwt-refresh-token-bundle": "^0.9.1",
        "knpuniversity/oauth2-client-bundle": "^1.34.0",
        "league/oauth2-google": "^3.0.2",
        "lexik/jwt-authentication-bundle": "^2.6",
        "minishlink/web-push": "^5.2.4",
        "nelmio/cors-bundle": "^2.2",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.2",
        "ramsey/uuid-doctrine": "^1.6.0",
        "symfony/asset": "^5.0.2",
        "symfony/console": "^5.1.2",
        "symfony/css-selector": "^5.1.2",
        "symfony/dom-crawler": "^5.1.2",
        "symfony/expression-language": "^5.0.2",
        "symfony/flex": "^1.8.4",
        "symfony/framework-bundle": "^5.1.2",
        "symfony/http-client": "^5.1.2",
        "symfony/lock": "^5.1.2",
        "symfony/mailer": "^5.1.2",
        "symfony/mercure-bundle": "^0.2.4",
        "symfony/monolog-bundle": "^3.5.0",
        "symfony/notifier": "^5.1.2",
        "symfony/property-access": "^5.0.2",
        "symfony/property-info": "^5.0.2",
        "symfony/security-bundle": "^5.0.2",
        "symfony/serializer": "^5.0.2",
        "symfony/slack-notifier": "^5.1.2",
        "symfony/translation": "^5.1.2",
        "symfony/twig-bundle": "^5.0.2",
        "symfony/validator": "^5.1.2",
        "symfony/webpack-encore-bundle": "^1.7.3",
        "symfony/yaml": "^5.1.2",
        "twig/cssinliner-extra": "^3.0.3",
        "twig/inky-extra": "^3.0.3"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.3.1",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-master",
        "symfony/browser-kit": "^5.0.2",
        "symfony/debug-bundle": "^5.0.2",
        "symfony/maker-bundle": "*",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/stopwatch": "^5.0.2",
        "symfony/web-profiler-bundle": "^5.0.2",
        "vimeo/psalm": "^3.11.6"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "ocramius/package-versions": true,
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "reset": [
            "bin/console doctrine:schema:drop --force",
            "bin/console doctrine:schema:create",
            "bin/console doctrine:fixtures:load --no-interaction",
            "bin/console heimdall:lock:create-table -d"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.0.2"
        }
    }
}
